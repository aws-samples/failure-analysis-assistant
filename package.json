{
  "name": "failure-analysis-assistant",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "infra": "npx ts-node --prefer-ts-exts bin/failure-analysis-assistant.ts"
  },
  "workspaces": [
    "lambda/functions/*",
    "lambda/lib"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "build:layer": "cd lambda/layers/nodejs && npm ci"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/node": "^20.12.8",
    "aws-cdk": "2.166.0",
    "esbuild": "^0.23.0",
    "eslint": "^9.7.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.0.0-alpha.10"
  },
  "dependencies": {
    "@cdklabs/generative-ai-cdk-constructs": "^0.1.288",
    "aws-cdk-lib": "2.166.0",
    "cdk-nag": "^2.28.154",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "lint-staged": {
    "*.(ts|mts)": [
      "npx eslint --fix"
    ],
    "*.(json|md)": [
      "npx prettier --write"
    ]
  },
  "license": "MIT-0"
}
